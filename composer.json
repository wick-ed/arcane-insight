{
  "name": "magenerds/arcane-insight",
  "description": "Magento site insights as a service",
  "version": "0.1.0",
  "require": {
    "appserver-io/routlt": "^2.1",
    "steverobbins/magescan": "^1.12",
    "jms/serializer": "0.16.0",
    "respect/validation": "~0.9"
  },
  "require-dev": {
    "doctrine/orm": "2.5.*",
    "appserver-io/phar": "^1.0",
    "appserver-io/robo-tasks": "^0.2",
    "appserver-io-psr/servlet": "^1.3",
    "appserver-io-psr/epb": "~2.0",
    "appserver-io-psr/mop": "~1.0",
    "appserver-io-psr/pms": "~1.0",
    "appserver-io/rmi": "~2.0",
    "appserver-io/http": "~2.0",
    "appserver-io/server": "~2.0",
    "appserver-io/messaging": "~2.0"
  },
  "autoload": {
    "psr-0": {
      "\\Magenerds\\ArcaneInsight": [
        "common/classes",
        "WEB-INF/classes",
        "META-INF/classes"
      ]
    }
  },
  "autoload-dev": {
    "psr-0": {
      "\\Magenerds\\ArcaneInsight": [
        "common/tests/",
        "WEB-INF/tests/",
        "META-INF/tests/"
      ]
    }
  }
}
